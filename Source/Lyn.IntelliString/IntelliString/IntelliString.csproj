    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>The Lyn</Company>
    <PackageProjectUrl>https://github.com/lynheo/IntelliString</PackageProjectUrl>
    <RepositoryUrl>https://github.com/lynheo/IntelliiString</RepositoryUrl>
